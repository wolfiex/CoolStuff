import time
setVisible(g.filter(degree>0))

def start(n,y):
  for i in xrange(n,y):
    print i
    runLayout(RandomLayout)
    stopLayout()
    runLayout(ForceAtlas2)
    time.sleep(70)
    stopLayout()
    exportGraph('/Users/wolfiex/Desktop/gephioutput/svg/%04d.svg'%i)
    exportGraph('/Users/wolfiex/Desktop/gephioutput/png/%04d.png'%i)
    time.sleep(60)
